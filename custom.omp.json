{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "#45F1C2",
					"style": "plain",
					"template": "<#64dff5>\ue285</> {{ .UserName }} \ue285",
					"type": "session"
				},
				{
					"foreground": "#14A5AE",
					"style": "plain",
					"template": " {{ .CurrentDate | date .Format }}",
					"properties": {
						"time_format": "2006/01/02-Mon-15:04:05"
					},
					"type": "time"
				},
				{
					"foreground": "#0d93aa",
					"style": "powerline",
					"properties": {
						"fetch_package_manager ": true
					},
					"template": "  {{ .Full }}",
					"type": "node"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#0CA0D8",
					"properties": {
						"folder_separator_icon": "\\",
						"style": "full"
					},
					"style": "plain",
					"template": "<#0CA0D8>\uf07b</> {{ .Path }} ",
					"type": "path"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#86BBD8",
					"powerline_symbol": "\ue0b0",
					"properties": {
						"fetch_upstream_icon": true
					},
					"style": "plain",
					"template": " {{ .UpstreamIcon }}{{ .HEAD }}",
					"type": "git"
				},
				{
					"foreground": "#86BBD8",
					"properties": {
						"fetch_stash_count": true
					},
					"style": "plain",
					"template": "{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
					"type": "git"
				},
				{
					"foreground": "#86BBD8",
					"style": "plain",
					"template": "\u2193{{ .Behind }} ",
					"type": "git"
				},
				{
					"foreground": "#86BBD8",
					"style": "plain",
					"template": "\u2191{{ .Ahead }} ",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"foreground": "#ff66a6",
					"style": "plain",
					"template": "\u2665 ",
					"type": "root"
				},
				{
					"foreground": "#DA627D",
					"style": "plain",
					"template": "\ue285 ",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"version": 2
}